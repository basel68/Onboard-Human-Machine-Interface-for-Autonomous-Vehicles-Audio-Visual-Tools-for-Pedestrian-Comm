#include "main.h"
#include "WS2812B_led.h"
static void WS2812B_spi(uint8_t color);

static void WS2812B_spi(uint8_t color){
	uint8_t data[24]=0;
	int index=0;

}
void WS2812B_send(led_struct_handle_t* led_handle){
	uint8_t leds_number=led_handle->led_number;
	while(leds_number>0){
		WS2812B_spi(led_handle->color);
		leds_number--;
	}
}

#ifndef INC_WS2812B_LED_H_
#define INC_WS2812B_LED_H_
typedef enum{
GREEN,
RED,
BLUE
}LED_COLOR;

typedef struct
{
	uint8_t led_number; //leds total count
	uint8_t color; //leds color
}led_struct_handle_t;
#endif /* INC_WS2812B_LED_H_ */
